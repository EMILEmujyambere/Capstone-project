<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.capstoneproject.elie_emile">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_elie_emile"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_elie_emile"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapstoneProject.NoActionBar">
        <activity android:name="com.capstoneproject.elie_emile.elie_emile_CuisineActivity" />
        <activity android:name="com.capstoneproject.elie_emile.elie_emile_DishActivity" />
        <activity android:name="com.capstoneproject.elie_emile.elie_emile_OrderDetail" />
        <activity android:name="com.capstoneproject.elie_emile.elie_emile_OrderActivity" />
        <activity android:name="com.capstoneproject.elie_emile.elie_emile_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>